<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:App/etc/crontab.xsd">
    <group id="default">
        <job name="vendor_feedback_email" instance="Vendor\FeedbackEmail\Cron\SendFeedbackEmail" method="execute">
            <schedule>0 0 * * *</schedule> <!-- This example runs the cron job daily at midnight -->
        </job>
    </group>
</config>
